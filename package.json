{
  "name": "bootstrap_native_test",
  "version": "1.0.0",
  "description": "Test bootstrap project wihtout jquery",
  "main": "server.js",
  "scripts": {
    "build:css": "lessc --clean-css --include-path=./node_modules/bootstrap/less:./node_modules/font-awesome/less --source-map-less-inline  --source-map ./less/index.less ./public/bundle.css",
    "build:js": "browserify --debug ./js/main.js --outfile=./public/bundle.js",
    "build": "npm run -s build:css & npm run -s build:js",
    "prestart": "npm run -s build"
  },
  "browserify": {
    "transform": [
      "vueify"
    ]
  },
  "author": "Peter Dodds",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^3.3.7",
    "bootstrap.native": "^2.0.19",
    "browserify": "^14.5.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "less": "^2.7.3",
    "less-plugin-clean-css": "^1.5.1",
    "vue": "^2.5.6",
    "vueify": "^9.4.1"
  }
}
